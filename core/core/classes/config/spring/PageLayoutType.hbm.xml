<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 	<class name="com.contento3.cms.page.layout.model.PageLayoutType" table="PAGE_LAYOUT_TYPE">
 	 	<id column="PAGE_LAYOUT_TYPE_ID" name="Id" type="java.lang.Integer">
			<generator class="increment"/>
 	 	</id>
	<property column="PAGE_LAYOUT_TYPE_NAME" name="name" type="java.lang.String"/>
	<property column="DESCRIPTION" name="description" type="java.lang.String"/>
	</class>
</hibernate-mapping>
